<h1>Login</h1>

<%= form_with scope: :login, url: '/login', local: true do |form| %>
  <p>
    <%= form.label :Email %><br>
    <%= form.text_field :Email %>
  </p>

  <p>
    <%= form.label :Senha %><br>
    <%= form.text_field :Senha %>
  </p>

  <p>
    <%= form.submit 'Entrar' %>
  </p>
<% end %>

<%= form_tag new_usuario_path, method: :get do %>
  <%= submit_tag 'Cadastrar-se' %>
<% end %>